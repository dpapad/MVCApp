<?xml version="1.0" encoding="UTF-8"?> 
<databaseChangeLog 
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd"> 
    <changeSet author="dev-dp" id="changelog-1.0">
        
        <createTable tableName="Contact">
            <column name="Id" type="int(11)" autoIncreament='true'>
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="FullName" type="varchar(45)" autoIncreament='false'>
                <constraints nullable="true" primaryKey="false"/>
                <column name="Nickname" type="varchar(45)" autoIncreament='false'>
                    <constraints nullable="true" primaryKey="false"/>
                </column>
                <column name="Notes" type="varchar(45)" autoIncreament='false'>
                    <constraints nullable="true" primaryKey="false"/>
                </column>
        </createTable>

    </changeSet>
</databaseChangeLog>
